{
  "name": "telegram-good-timing-bot",
  "version": "0.0.1",
  "description": "It is a [Telegram](https://telegram.org/) Bot to help you find a good timing to do things.",
  "main": "index.js",
  "dependencies": {
    "body-parser": "^1.16.0",
    "express": "^4.14.1",
    "mongoose": "^4.8.1",
    "node-telegram-bot-api": "^0.21.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "sinon": "^2.1.0"
  },
  "scripts": {
    "start": "export $(cat .env | xargs) && node index.js",
    "setup-webhook": "export $(cat .env | xargs) && node webhook.js setup",
    "remove-webhook": "export $(cat .env | xargs) && node webhook.js remove",
    "test": "export $(cat .env | xargs) && mocha"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/johnlinp/telegram-good-timing-bot.git"
  },
  "author": "John Lin",
